<?xml version="1.0"?>

<robot name="si_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find si_bot)/urdf/si_bot.gazebo" />


  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>


  <!-- ===== Chassis ===== -->

  <link name="chassis">
    <pose>0 0 0.1 0 0 0</pose> <!-- 0 0 0.1 0 0 0 -->
    <inertial>
      <mass value="15.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision name="chassis_collision">
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".35 .35 .05"/>
      </geometry>
    </collision>
    <visual name="chassis_visual">
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".35 .35 .05"/>
      </geometry>
    </visual>
  </link>

  <joint name="back_caster_wheel_fixed" type="fixed">
    <origin xyz="-0.1 0.0 -0.025" rpy="0 0 0"/>
    <child link="back_caster_wheel"/>
    <parent link="chassis" />
  </joint>
  <link name="back_caster_wheel">
      <inertial>
        <mass value="0.1"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-6" ixy="0" ixz="0"
            iyy="1e-6" iyz="0"
            izz="1e-6"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.05"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <sphere radius="0.05"/>
        </geometry>
      </visual>
  </link>


  <joint name="dome0_fixed" type="fixed">
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
    <child link="dome0"/>
    <parent link="chassis" />
  </joint>
  <link name="dome0">
      <inertial>
        <mass value="0.1"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-6" ixy="0" ixz="0"
            iyy="1e-6" iyz="0"
            izz="1e-6"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.3 0.3 0.05"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.3 0.3 0.05"/>
        </geometry>
      </visual>
  </link>


  <!-- head stand -->

  <joint name="stand_fixed" type="fixed">
    <origin xyz="0.1 0 0.040" rpy="0 0 0"/>
    <child link="stand"/>
    <parent link="dome0" />
  </joint>
  <link name="stand">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-7" ixy="0" ixz="0"
            iyy="1e-7" iyz="0"
            izz="1e-7"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder radius=".025" length="0.03"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <cylinder radius=".025" length="0.03"/>
        </geometry>
      </visual>
  </link>


  <joint name="dome1_fixed" type="fixed">
    <origin xyz="0 0 0.04" rpy="0 0 0"/>
    <child link="dome1"/>
    <parent link="stand" />
  </joint>
  <link name="dome1">
      <inertial>
        <mass value="0.1"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-6" ixy="0" ixz="0"
            iyy="1e-6" iyz="0"
            izz="1e-6"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.1 0.15 0.05"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.1 0.15 0.05"/>
        </geometry>
      </visual>
  </link>

  <!-- ======= Ears ================= -->

  <joint name="left_ear_fixed" type="fixed">
    <origin xyz="0.02 0.075 0" rpy="0 0 0"/>
    <child link="left_ear"/>
    <parent link="dome1" />
  </joint>
  <link name="left_ear">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-7" ixy="0" ixz="0"
            iyy="1e-7" iyz="0"
            izz="1e-7"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.025" length="0.025"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.025" length="0.025"/>
        </geometry>
      </visual>
  </link>

  <joint name="right_ear_fixed" type="fixed">
    <origin xyz="0.02 -0.075 0" rpy="0 0 0"/>
    <child link="right_ear"/>
    <parent link="dome1" />
  </joint>
  <link name="right_ear">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-7" ixy="0" ixz="0"
            iyy="1e-7" iyz="0"
            izz="1e-7"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.025" length="0.025"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.025" length="0.025"/>
        </geometry>
      </visual>
  </link>

  <!-- ======= Eyes =============== -->

  <joint name="left_eye_fixed" type="fixed">
    <origin xyz="0.05 0.05 0" rpy="0 0 0"/>
    <child link="left_eye"/>
    <parent link="dome1" />
  </joint>
  <link name="left_eye">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-7" ixy="0" ixz="0"
            iyy="1e-7" iyz="0"
            izz="1e-7"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.012" length="0.01"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.012" length="0.01"/>
        </geometry>
      </visual>
  </link>


  <joint name="right_eye_fixed" type="fixed">
    <origin xyz="0.05 -0.05 0" rpy="0 0 0"/>
    <child link="right_eye"/>
    <parent link="dome1" />
  </joint>
  <link name="right_eye">
      <inertial>
        <mass value="0.01"/>
        <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
        <inertia
            ixx="1e-7" ixy="0" ixz="0"
            iyy="1e-7" iyz="0"
            izz="1e-7"
        />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.01" length="0.01"/>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 1.5707 0"/>
        <geometry>
          <cylinder radius="0.01" length="0.01"/>
        </geometry>
      </visual>
  </link>




  <!-- ====== Wheels ============= -->

  <joint name="left_wheel_hinge" type="continuous">
    <origin xyz="0.1 0.13 -0.025" rpy="0 0 0"/>
    <child link="left_wheel"/>
    <parent link="chassis" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>
  <link name="left_wheel">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius=".05" length="0.05"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.05"/>
      </geometry>
    </visual>
  </link>


  <joint name="right_wheel_hinge" type="continuous">
    <origin xyz="0.1 -0.13 -0.025" rpy="0 0 0"/>
    <child link="right_wheel"/>
    <parent link="chassis" />
    <axis xyz="0 1 0" rpy="0 0 0" />
    <limit effort="10000" velocity="1000" />
    <joint_properties damping="1.0" friction="1.0" />
  </joint>
  <link name="right_wheel">
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius=".05" length="0.05"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="1.5707 0 0"/>
      <geometry>
        <cylinder radius="0.05" length="0.05"/>
      </geometry>
    </visual>
  </link>


  <!-- ====== Camera ============= -->

  <joint name="camera_joint" type="fixed">
    <origin xyz="0.135 0 0.03" rpy="0 0 0" />
    <child link="camera" />
    <parent link="chassis" />
  </joint>
  <link name="camera">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
    <collision>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05" />
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.05" />
      </geometry>
    </visual>
  </link>

  <!-- Fix for rotated point cloud -->
  <joint name="camera_link_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707" />
    <child link="camera_link" />
    <parent link="camera" />
  </joint>
  <link name="camera_link"></link>


